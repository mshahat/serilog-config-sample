{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "System": "Warning"
      }
    },
    "Using": [ "Serilog.Sinks.Console" ],
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "[{Timestamp} {Level:u4}] {Message:l} {Properties}{NewLine}{Exception}"
        }
      }
    ],
    "Properties":  { "Application":  "serilog-config-sample"}
  },
  "AllowedHosts": "*"
}
