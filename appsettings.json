{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Error",
        "Microsoft.Hosting.Lifetime": "Error",
        "System": "Error"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "[{Timestamp} {Level:u4}] {Message:l} {Properties}{NewLine}{Exception}"
        }
      },
      {
        "Name": "Elasticsearch",
        "Args": {
          "nodeUris": "https://46f9515c2b884d848c015760fa96faf5.francecentral.azure.elastic-cloud.com:9243",
          // Specify Environment
          "indexFormat": "gs1eg-logs-development-{0:yyyy.MM.dd}",
          "connectionGlobalHeaders": "Authorization=ApiKey QVJVNjlYd0IwcktobloxMmF2ZUg6WWNmWlRvODNRdFdLcFJ3N3FhUUVIdw=="
        }
      }
    ],
    "Using": [ "Serilog.Enrichers.ClientInfo" ],
    "Enrich": [ "FromLogContext", "WithClientAgent" ],
    "Properties": { "Application": "serilog-config-sample-from-appsettings" }
  },
  "AllowedHosts": "*"
}
