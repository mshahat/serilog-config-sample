{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "[{Timestamp} {Level:u4}] {Message:l} {Properties}{NewLine}{Exception}"
        }
      },
      {
        "Name": "Elasticsearch",
        "Args": {
          "nodeUris": "https://my-deployment-5ff2a5.es.francecentral.azure.elastic-cloud.com:9243",
          "connectionGlobalHeaders": "Authorization=ApiKey RFcwNFBId0JSSXVsZm41ZFFsM006TUVNOXdBd1dRTXVDVGlHT0hqcGNmdw=="
        }
      }
    ],
    "Using": [ "Serilog.Enrichers.ClientInfo" ],
    "Enrich": [ "FromLogContext", "WithClientAgent" ],
    "Properties": { "Application": "serilog-config-sample-from-appsettings" }
  },
  "AllowedHosts": "*"
}
